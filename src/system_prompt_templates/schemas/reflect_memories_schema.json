{
  "output_format": {
    "type": "array",
    "items": {
      "type": "object",
      "required": ["lesson", "importance", "category", "thoughts"],
      "properties": {
        "lesson": {
          "type": "string",
          "description": "The comprehensive lesson learned from the interaction"
        },
        "importance": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "description": "Importance level of the lesson (0-1)"
        },
        "category": {
          "type": "string",
          "enum": [
            "tools",
            "agentic structure",
            "giving feedback",
            "memory reflection",
            "long term memory transfer",
            "thought loop",
            "reasoning",
            "self improvement",
            "insight"
          ]
        },
        "thoughts": {
          "type": "string",
          "description": "Analysis of the memory and its usefulness"
        }
      }
    }
  }
}